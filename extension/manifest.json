{
  "manifest_version": 2,
  "name": "Lovely for Trello",
  "version": "1.0.7",
  "icons": {
    "128": "trello-lovely-128.png",
    "48": "trello-lovely-48.png"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [{
      "matches": ["https://trello.com/*"],
      "js": ["jquery-1.7.2.min.js", "trello-lovely.js"],
      "css": ["trello-lovely.css"]
  }]
}
